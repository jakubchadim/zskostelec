    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --ignore-pattern 'public/*' --ignore-pattern '__mocks__/*'",
    "format": "eslint '*/**/*.{js,ts,tsx}' --ignore-pattern 'public/*' --ignore-pattern '__mocks__/*' --quiet --fix",
